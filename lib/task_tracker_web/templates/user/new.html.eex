<h1>New User</h1>

<%= render "form.html", Map.put(assigns, :action, Routes.user_path(@conn, :create)) %>

<%= if @current_user do %>
	<span><%= link "Users (don't save)", to: Routes.user_path(@conn, :index),  class: "btn btn-primary" %></span>
<% else %>
		<span><%= link "Home", to: Routes.page_path(@conn, :index),  class: "btn btn-primary" %></span>
<% end %>